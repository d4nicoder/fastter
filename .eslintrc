{
  "env": {
    "es6": true,
    "jest/globals": true
  },
  "plugins": [
    "jest"
  ],
  "extends": "standard",
  "globals": {},
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "no-unused-expressions": 0,
    "jest/no-focused-tests": "error",
    "jest/no-disabled-tests": "error",
    "jest/no-standalone-expect": "error",
    "jest/no-commented-out-tests": "warn"
  }
}